generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("POSTGRES_PRISMA_URL")
    directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Chat {
    id       String @id @default(uuid())
    name     String @default("New Chat")
    userId   String
    messages Json[]
}
